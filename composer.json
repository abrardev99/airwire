{
    "name": "archtechx/airwire",
    "description": "A lightweight full-stack component layer that doesn't dictate your front-end framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Å tancl",
            "email": "samuel@archte.ch"
        }
    ],
    "autoload": {
        "psr-4": {
            "Airwire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Airwire\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.2",
        "illuminate/testing": "^8.42",
        "orchestra/testbench": "^7.0|^6.17",
        "pestphp/pest-plugin-laravel": "^1.0.0"
    },
    "require": {
        "illuminate/support": "^9.0|^8.42",
        "illuminate/console": "^9.0|^8.42"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Airwire\\AirwireServiceProvider"
            ]
        }
    }
}
